<?xml version='1.0' encoding='utf-8'?>
<widget id="com.titanio.cryptar" version="8.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Cryptar Chat</name>
    <description>
		Cryptar - by Sistemas Titanio
    </description>
    <author email="info@sistemastitanio.com.ar" href="http://sistemastitanio.com.ar">
        Sistemas Titanio
    </author>
    <preference name="android-minSdkVersion" value="19" />
    <preference name="android-build-tool" value="gradle" />
    <preference name="phonegap-version" value="cli-8.1.1" />
    <content src="index.html" />
    <preference name="prerendered-icon" value="true" />
    <platform name="ios">
        <splash height="1136" platform="ios" src="www/res/screen/ios/Default-568h@2x~iphone.png" width="640" />
        <splash height="1334" platform="ios" src="www/res/screen/ios/Default-667h.png" width="750" />
        <splash height="2208" platform="ios" src="www/res/screen/ios/Default-736h.png" width="1242" />
        <splash height="960" platform="ios" src="www/res/screen/ios/Default@2x~iphone.png" width="640" />
        <splash height="480" platform="ios" src="www/res/screen/ios/Default~iphone.png" width="320" />
        <icon height="57" src="www/res/icon/ios/icon.jpg" width="57" />
        <icon height="60" src="www/res/icon/ios/icon-60.jpg" width="60" />
        <icon height="120" src="www/res/icon/ios/icon-60@2x.jpg" width="120" />
        <icon height="180" src="www/res/icon/ios/icon-60@3x.jpg" width="180" />
        <icon height="80" src="www/res/icon/ios/icon-40@2x.jpg" width="80" />
        <icon height="50" src="www/res/icon/ios/icon-50.jpg" width="50" />
        <icon height="100" src="www/res/icon/ios/icon-50@2x.jpg" width="100" />
        <icon height="72" src="www/res/icon/ios/icon-72.jpg" width="72" />
        <icon height="144" src="www/res/icon/ios/icon-72@2x.jpg" width="144" />
        <allow-navigation href="https://cryptar.com.ar" />
        <allow-navigation href="https://cryptarchat.cryptar.com.ar" />
        <allow-navigation href="https://*" />
        <allow-intent href="https://*" />
        <allow-intent href="wss:*" />
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <access origin="https://cryptarchat.cryptar.com.ar/*" />
        <access origin="https://cryptar.com.ar/*" />
        <access origin="http://localhost/*" />
        <access origin="*" />
        <edit-config file="*-Info.plist" mode="merge" target="UIStatusBarHidden">
            <true />
        </edit-config>
        <edit-config file="*-Info.plist" mode="merge" target="UIViewControllerBasedStatusBarAppearance">
            <false />
        </edit-config>
    </platform>
    <allow-navigation href="http://*" />
    <allow-navigation href="https://*" />
    <allow-navigation href="http://localhost" />
    <allow-intent href="https://*" />
    <allow-intent href="http://localhost" />
    <allow-intent href="wss:*" />
    <allow-intent href="tel:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <feature name="CDVWKWebViewEngine">
        <param name="ios-package" value="CDVWKWebViewEngine" />
    </feature>
    <preference name="CordovaWebViewEngine" value="CDVWKWebViewEngine" />
    <preference name="iosPersistentFileLocation" value="Library" />
    <preference name="iosExtraFilesystems" value="library,library-nosync,documents,documents-nosync,cache,bundle,root" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="KeyboardShrinksView" value="true" />
    <feature name="http://api.phonegap.com/1.0/network" />
    <feature name="http://api.phonegap.com/1.0/device" />
    <feature name="http://api.phonegap.com/1.0/camera" />
    <feature name="Device">
        <param name="ios-package" value="CDVDevice" />
    </feature>
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <access origin="https://cryptarchat.cryptar.com.ar" />
    <access origin="https://cryptar.com.ar" />
    <access origin="*" />
    <plugin name="cordova-plugin-battery-status" spec="~1.1.1" />
    <plugin name="cordova-plugin-camera" spec="~2.1.1" />
    <plugin name="cordova-plugin-media-capture" spec="^1.4.3">
        <variable name="CAMERA_USAGE_DESCRIPTION" value=" " />
        <variable name="MICROPHONE_USAGE_DESCRIPTION" value=" " />
        <variable name="PHOTOLIBRARY_USAGE_DESCRIPTION" value=" " />
    </plugin>
    <plugin name="cordova-plugin-console" spec="~1.0.2" />
    <plugin name="cordova-plugin-contacts" spec="~2.0.1" />
    <plugin name="cordova-plugin-device" spec="~1.1.1" />
    <plugin name="cordova-plugin-device-motion" spec="~1.2.0" />
    <plugin name="cordova-plugin-device-orientation" spec="~1.0.2" />
    <plugin name="cordova-plugin-file" spec="~4.1.1" />
    <plugin name="cordova-plugin-globalization" spec="~1.0.3" />
    <plugin name="cordova-plugin-inappbrowser" spec="~1.3.0" />
    <plugin name="cordova-plugin-network-information" spec="~1.2.0" />
    <plugin name="cordova-plugin-statusbar" spec="~2.1.2" />
    <plugin name="cordova-plugin-vibration" spec="~2.1.0" />
    <plugin name="cordova-plugin-prevent-screenshot" spec="^1.0.3" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.4" />
    <plugin name="cordova-plugin-dialogs" spec="^2.0.2" />
    <plugin name="cordova-plugin-keyboard" spec="1.2.0" />
    <plugin name="pushbots-cordova-plugin" spec="^1.6.14" />
</widget>
